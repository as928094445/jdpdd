
.container {
		padding-bottom: env(safe-area-inset-bottom);
}
.tui-list{font-size:24rpx;}
.tui-box {
    	background: #fff;
    	box-shadow: 0 3rpx 20rpx rgba(183, 183, 183, 0.1);
    	border-radius: 10rpx;
    	overflow: hidden;
        margin: 20rpx;
}
.tui-tool-box {
    	margin-top: 20rpx;
}
.tui-cell-header {
    	width: 100%;
    	height: 74rpx;
    	padding: 0 26rpx;
    	box-sizing: border-box;
    	display: -webkit-box;
    	display: -webkit-flex;
    	display: flex;
    	-webkit-box-align: center;
    	-webkit-align-items: center;
    	        align-items: center;
    	-webkit-box-pack: justify;
    	-webkit-justify-content: space-between;
    	        justify-content: space-between;
}
.tui-cell-title {
    	font-size: 30rpx;
    	line-height: 30rpx;
    	color: #333;
}
.tui-cell-sub {
    	font-size: 26rpx;
    	font-weight: 400;
    	color: #999;
    	padding-right: 28rpx;
}
.tui-wealth-desc{
        font-size: 24rpx;
        padding: 20rpx;
        line-height: 42rpx;
}
.tui-line-cell {
    	width: 100%;
    	box-sizing: border-box;
    	display: -webkit-box;
    	display: -webkit-flex;
    	display: flex;
    	-webkit-box-align: center;
    	-webkit-align-items: center;
    	        align-items: center;
}
.tui-title {
        font-size:24rpx;
    	line-height: 32rpx;
    	-webkit-flex-shrink: 0;
    	        flex-shrink: 0;
}
.tui-input {
    	font-size: 32rpx;
    	color: #333;
    	padding-left: 20rpx;
    	-webkit-box-flex: 1;
    	-webkit-flex: 1;
    	        flex: 1;
}
.phcolor{font-size:24rpx;}
.tui-btn-box{padding:20rpx;font-size: 24rpx;}
.tui-btn-danger{background: #EB0909 !important;color: #fff;}
.tui-btn-mini{font-size:26rpx;}
.tui-list-cell-name {
        font-size: 24rpx;
		padding-left: 20rpx;
		vertical-align: middle;
		line-height: 30rpx;
}
.tui-list::after {
		left: 94rpx !important
}
.tui-right {
		margin-left: auto;
		margin-right: 34rpx;
		font-size: 26rpx;
		line-height: 1;
		color: #999;
}
.logo {
		height: 40rpx;
		width: 40rpx;
}
.tui-flex {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.tui-msg-box {
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.tui-msg-pic {
		width: 100rpx;
		height: 100rpx;
		border-radius: 50%;
		display: block;
		margin-right: 24rpx;
}
.tui-msg-item {
		max-width: 500rpx;
		min-height: 80rpx;
		overflow: hidden;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
}
.tui-msg-name {
		overflow: hidden;
		white-space: nowrap;
		text-overflow: ellipsis;
		font-size: 34rpx;
		line-height: 1;
		color: #262b3a;
}
.tui-msg-content {
		overflow: hidden;
		white-space: nowrap;
		text-overflow: ellipsis;
		font-size: 26rpx;
		line-height: 1;
		color: #9397a4;
}
.tui-msg-right {
		max-width: 120rpx;
		height: 88rpx;
		margin-left: auto;
		text-align: right;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		        flex-direction: column;
		-webkit-box-pack: justify;
		-webkit-justify-content: space-between;
		        justify-content: space-between;
		-webkit-box-align: end;
		-webkit-align-items: flex-end;
		        align-items: flex-end;
}
.tui-msg-right.tui-right-dot {
		height: 76rpx;
		padding-bottom: 10rpx;
}
.tui-msg-time {
		width: 100%;
		font-size: 24rpx;
		line-height: 24rpx;
		color: #9397a4;
}
.tui-badge {
		width: auto
}
.tui-msg::after {
		left: 154rpx !important
}
.tui-tips {
    	padding: 30rpx;
    	color: #999;
    	font-size: 24rpx;
}

